{
	"name": "sactory",
	"version": "0.91.0",
	"description": "HTML-spiced javascript for DOM creation and manipulation",
	"main": "index.js",
	"scripts": {
		"dist": "gulp dist",
		"compile": "node ./src/watcher.js compile",
		"watch": "node ./src/watcher.js watch",
		"test:compile": "npm run compile -- ./test/ ./test/dist/",
		"test:run": "node ./test/index.js",
		"test": "npm run test:compile && npm run test:run",
		"unittest": "node ./unittest.js",
		"unittest-open": "npm run unittest && start ./unittest/index.html"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sactory/sactory.git"
	},
	"keywords": [
		"factory",
		"html",
		"dom"
	],
	"author": "Mark White",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sactory/sactory/issues"
	},
	"homepage": "https://github.com/sactory/sactory",
	"devDependencies": {
		"event-stream": "^4.0.1",
		"express": "^4.17.0",
		"gulp": "^4.0.2",
		"gulp-clone": "^2.0.1",
		"gulp-concat": "^2.6.1",
		"gulp-footer": "^2.0.2",
		"gulp-header": "^2.0.7",
		"gulp-nop": "0.0.3",
		"gulp-rename": "^1.4.0",
		"gulp-replace": "^1.0.0",
		"gulp-uglify": "^3.0.2",
		"node-fetch": "^2.6.0"
	}
}
