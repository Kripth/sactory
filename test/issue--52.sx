function test({size}) {
	return <div>
		<:inherit &display="inline-block" &{width, height}=`${size}px`>
			foreach(to 5) {
				<div &background=Sactory.css.rgb() />
			}
		</:inherit>
	</div>
}

<:this>
	<[test] $size=1 />
	<[test] $size=2 />
	<[test] $size=4 />
	<[test] $size=8 />
	<[test] $size=16 />
	<[test] $size=32 />
	<[test] $size=64 />
	<[test] $size=128 />
</:this>
